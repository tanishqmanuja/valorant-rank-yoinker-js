{
  "name": "vryjs",
  "version": "0.1.1",
  "description": "VALORANT Rank Yoinker JS",
  "repository": "tanishqmanuja/valorant-rank-yoinker-js",
  "type": "module",
  "files": [
    "out"
  ],
  "bin": {
    "vryjs": "out/vryjs.mjs"
  },
  "scripts": {
    "start": "node --no-warnings dist/index.mjs",
    "start:dev": "tsx --no-warnings src/index.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build:dev": "tsx scripts/build.ts",
    "build:prod": "tsx scripts/build.ts --prod",
    "build": "pnpm build:prod",
    "type:check": "tsc --noEmit",
    "release": "release-it"
  },
  "keywords": [
    "valorant",
    "riot",
    "cli"
  ],
  "author": "tanishqmanuja",
  "license": "MIT",
  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.10.7",
    "@types/yargs": "^17.0.32",
    "esbuild": "^0.19.11",
    "execa": "^8.0.1",
    "prettier": "^3.1.1",
    "release-it": "^17.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@tqman/axios-interceptor-suite": "^0.3.4",
    "@tqman/valoffi-api-client": "^1.0.2",
    "@tqman/valorant-api-client": "^1.3.3",
    "@tqman/valorant-api-types": "^0.8.1",
    "@types/dependency-solver": "^1.0.3",
    "axios": "^1.6.5",
    "axios-logger": "^2.7.1",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "change-case": "^5.4.0",
    "cli-cursor": "^4.0.0",
    "cli-table3": "^0.6.3",
    "date-fns": "^3.2.0",
    "dependency-solver": "^1.0.6",
    "lowdb": "^7.0.1",
    "ohash": "^1.1.3",
    "ora": "^8.0.1",
    "rxjs": "^7.8.1",
    "string-width": "^7.0.0",
    "ts-pattern": "^5.0.6",
    "tty-events": "^1.0.0",
    "turbo": "^1.11.3",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "yaml": "^2.3.4",
    "yargs": "17.7.2",
    "zod": "^3.22.4"
  }
}
